    "eslint-plugin-react-hooks": "4.0.4",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "tailwindcss": "1.4.6",
    "twin.macro": "1.3.0",
    "typescript": "3.9.5",
    "typescript-plugin-tw-template": "2.0.1",
